    "element-plus": "^1.1.0-beta.24",
    "mitt": "^3.0.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.0-rc.15",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "^3.2.20",
    "autoprefixer": "^10.3.7",
    "plop": "^2.7.6",
    "release-it": "^14.11.6",
    "sass": "^1.43.4",
    "unplugin-vue-components": "^0.16.0",
    "vite": "^2.6.12",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^2.1.1"
  },
  "release-it": {
    "npm": {
      "publish": false
